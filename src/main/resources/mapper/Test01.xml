<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.test01.repository.Test01Repository">
    <delete id="deleteTgt" >
        delete from tgt_test
    </delete>
    
    <insert id="insertTgt" parameterType="com.example.demo.test01.domain.Test01" >
        insert into tgt_test
        (
        id
        ,active
        ,e_mail
        ,name
        ,zip_code
        )values(
        #{id}
        ,#{active}
        ,#{eMail}
        ,#{name}
        ,#{zipCode}
        )
    </insert>
</mapper>

